<settings>
	<!-- ======================================================================== -->
	<!--  Accounts for SCM ONLY                                                   -->
	<!-- ======================================================================== -->
	<localRepository>d:/repository</localRepository>
	<servers>
	<server>
      <id>snapshots</id>
      <username>dev</username>
      <password>dev123</password>
    </server>
	<server>
      <id>releases</id>
      <username>dev</username>
      <password>dev123</password>
    </server>
    <server>
      <id>Central</id>
      <username>dev</username>
      <password>dev123</password>
    </server>
	</servers>

    <mirrors>
        <mirror>
      <id>yingzimaven</id>
      <name>yingzi maven</name>
      <url>http://maven.yingzidt.com:9091/nexus/content/groups/public/</url>
      <mirrorOf>central</mirrorOf> 
    </mirror>  

    
  </mirrors>

	<!-- ======================================================================== -->
	<!--  Profiles                                                                -->
	<!-- ======================================================================== -->
	<profiles>
    <profile>
      <id>yingzi-dev</id>
      <activation>
        <activeByDefault>true</activeByDefault>
        <property>
          <name>yingzi-dev</name>
        </property>
      </activation>
        <properties>  
          <deploy.env>dev</deploy.env>
          <deploy.repository.id>snapshots</deploy.repository.id>
          <deploy.repository.url>http://maven.yingzidt.com:9091/nexus/content/repositories/snapshots/</deploy.repository.url>
        </properties> 
      <repositories>
        <repository>
          <releases>
            <enabled>true</enabled>
          </releases>
          <id>releases</id>
          <name>YINGZI Releases Repositories</name>
          <url>http://maven.yingzidt.com:9091/nexus/content/repositories/releases/</url>
        </repository>
		<repository>
          <id>snapshots</id>
          <name>YINGZI snapshots Repositories</name>
          <url>http://maven.yingzidt.com:9091/nexus/content/repositories/snapshots/</url>
        </repository>
        <repository>
          <id>Central</id>
          <name>YINGZI Central Repositories</name>
          <url>http://maven.yingzidt.com:9091/nexus/content/repositories/Central/</url>
        </repository>
      </repositories>
    </profile> 


	</profiles>

	<pluginGroups>

		
	</pluginGroups>
	<activeProfiles>
            <activeProfile>yingzi-dev</activeProfile> 
	</activeProfiles>
</settings>
